<h1>Geography Quiz: <%= @quiz.title %></h1>
<div id="quiz-container">
  <% @countries.each do |country| %>
    <div class="country" data-name="<%= country.name %>" data-population="<%= country.population %>">
      <%= country.name %>
    </div>
  <% end %>
</div>
<button id="submit-sort">Submit Order</button>
